# aarch64 (16kb page size)
export XBPS_LTO_STYLE=full
XBPS_CFLAGS="-Os -mcpu=native -march=native -fno-plt -pipe -U_FORTIFY_SOURCE -D_FORTIFY_SOURCE=3 -fstack-protector-strong -mbranch-protection=standard -flto=auto -g0"
XBPS_CXXFLAGS="${XBPS_CFLAGS}"
XBPS_LDFLAGS="-Wl,-s -Wl,-z,relro,-z,now -Wl,--gc-sections -Wl,--as-needed -Wl,-z,max-page-size=16384 -flto=auto"
